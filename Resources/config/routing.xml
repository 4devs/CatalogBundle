<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="f_devs_catalog_list" path="/list/{type}">
        <default key="_controller">f_devs_catalog.controller.default:listAction</default>
    </route>
    <route id="f_devs_catalog_item" path="/item/{id}">
        <default key="_controller">f_devs_catalog.controller.default:itemAction</default>
    </route>
</routes>
